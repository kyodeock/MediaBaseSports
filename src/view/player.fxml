<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="770.0" prefWidth="1850.0"
	stylesheets="/view/playerinfostyle.css"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="control.PlayerInfoController">
	<children>
		<Label fx:id="lbDiv" />
		<HBox>
			<padding>
				<Insets top="10.0" bottom="10.0" />
			</padding>
			<children>
				<VBox alignment="CENTER">
					<children>
						<Button fx:id="btnLogo">
							<graphic>
								<VBox alignment="CENTER" prefHeight="200.0"
									prefWidth="100.0">
									<children>
										<ImageView fx:id="imgLogo">
											<image>
												<Image url="/view/backgroundImg.png" />
											</image>
										</ImageView>
										<Label text="초기화하시려면 로고를 클릭하세요" />
									</children>
								</VBox>
							</graphic>
						</Button>
					</children>
				</VBox>
				<VBox alignment="CENTER">
					<children>
						<HBox alignment="CENTER">
							<padding>
								<Insets bottom="5.0" left="10.0" right="10.0" />
							</padding>
							<spacing>20.0</spacing>
							<children>
								<Label prefWidth="100.0" text="이름 : " />
								<TextField fx:id="txtName" prefWidth="150.0"
									promptText="이름" />
								<Label prefWidth="100.0" text="성별 : " />
								<ComboBox fx:id="cbGender" prefWidth="150.0"
									promptText="성별" />
								<Label prefWidth="100.0" text="선수 구분 : " />
								<ComboBox fx:id="cbDivision" prefWidth="150.0"
									promptText="구분" />
							</children>
						</HBox>
						<HBox alignment="CENTER">
							<padding>
								<Insets bottom="5.0" left="10.0" right="10.0" />
							</padding>
							<spacing>20.0</spacing>
							<children>
								<Label prefWidth="100.0" text="생년월일 : " />
								<DatePicker fx:id="dpBirth" prefWidth="150.0" />
								<Label prefWidth="100.0" text="신장 : " />
								<TextField fx:id="txtHeight" prefWidth="150.0"
									promptText="신장" />
								<Label prefWidth="100.0" text="체중 : " />
								<TextField fx:id="txtWeight" prefWidth="150.0"
									promptText="체중" />
							</children>
						</HBox>
						<HBox alignment="CENTER">
							<padding>
								<Insets bottom="5.0" left="10.0" right="10.0" />
							</padding>
							<spacing>20.0</spacing>
							<children>
								<Label prefWidth="100.0" text="연락처 : " />
								<TextField fx:id="txtPhone" prefWidth="150.0"
									promptText="연락처" />
								<Label prefWidth="100.0" text="이메일 : " />
								<TextField fx:id="txtEmail" prefWidth="150.0"
									promptText="이메일" />
								<Label prefWidth="100.0" text="" />
								<Label prefWidth="150.0" text="" />
							</children>
						</HBox>
						<HBox alignment="CENTER">
							<padding>
								<Insets bottom="5.0" left="10.0" right="10.0" />
							</padding>
							<spacing>20.0</spacing>
							<children>
								<Separator prefHeight="0.0" prefWidth="690.0" />
							</children>
						</HBox>
						<HBox alignment="CENTER">
							<padding>
								<Insets bottom="5.0" left="10.0" right="10.0" />
							</padding>
							<spacing>20.0</spacing>
							<children>
								<Label prefWidth="100.0" text="국적 : " />
								<TextField fx:id="txtNation" prefWidth="150.0"
									promptText="국적" />
								<Label prefWidth="100.0" text="비자 : " />
								<ComboBox fx:id="cbVisa" prefWidth="150.0"
									promptText="선택" />
								<Label prefWidth="100.0" text="포지션 : " />
								<ComboBox fx:id="cbPosition" prefWidth="150.0"
									promptText="선택" />
							</children>
						</HBox>
						<HBox alignment="CENTER">
							<padding>
								<Insets bottom="5.0" left="10.0" right="10.0" />
							</padding>
							<spacing>20.0</spacing>
							<children>
								<Label prefWidth="100.0" text="주발 : " />
								<ComboBox fx:id="cbPfoot" prefWidth="150.0"
									promptText="선택" />
								<Label prefWidth="100.0" text="현소속구단 : " />
								<ComboBox fx:id="cbAttached" prefWidth="150.0"
									promptText="선택" />
								<Label prefWidth="100.0" text="통산출장수 : " />
								<TextField fx:id="txtTotApps" prefWidth="150.0"
									promptText="통산총출장수" />

							</children>
						</HBox>
						<HBox alignment="CENTER">
							<padding>
								<Insets bottom="5.0" left="10.0" right="10.0" />
							</padding>
							<spacing>20.0</spacing>
							<children>
								<Label prefWidth="100.0" text="통산득점수 : " />
								<TextField fx:id="txtTotScores" prefWidth="150.0"
									promptText="통산총득점수" />
								<Label prefWidth="100.0" text="통산도움수 : " />
								<TextField fx:id="txtTotAssists" prefWidth="150.0"
									promptText="통산총도움수" />
								<Label prefWidth="100.0" text="통산평점 : " />
								<TextField fx:id="txtAvgRate" prefWidth="150.0"
									promptText="통산평균평점" />
							</children>
						</HBox>
						<HBox alignment="CENTER">
							<padding>
								<Insets bottom="5.0" left="10.0" right="10.0" />
							</padding>
							<spacing>20.0</spacing>
							<children>
								<Label prefWidth="100.0" text="거주지 : " />
								<TextField fx:id="txtAddress" prefWidth="730.0"
									promptText="거주지" />
							</children>
						</HBox>
						<HBox alignment="CENTER">
							<padding>
								<Insets bottom="5.0" left="10.0" right="10.0" />
							</padding>
							<spacing>20.0</spacing>
							<children>
								<Label prefWidth="100.0" text="바라는점 : " />
								<TextField fx:id="txtHtA" prefWidth="730.0"
									promptText="에이전트에게 바라는 점" />
							</children>
						</HBox>
					</children>
				</VBox>
				<VBox prefHeight="230.0" prefWidth="240.0">
					<padding>
						<Insets bottom="5.0" />
					</padding>
					<spacing>5.0</spacing>
					<children>
						<ImageView fx:id="imgPlayer">
							<image>
								<Image url="/view/NoImg.png" />
							</image>
						</ImageView>
						<HBox>
							<children>
								<TextField fx:id="txtPath" prefWidth="220.0"
									promptText="이미지경로" />
							</children>
						</HBox>
					</children>
				</VBox>
				<VBox prefHeight="230.0" prefWidth="410.0">
					<children>
						<HBox alignment="CENTER" prefHeight="120.0" prefWidth="410.0">
							<padding>
								<Insets bottom="10.0" />
							</padding>
							<spacing>30.0</spacing>
							<children>
								<Button fx:id="btnRegist">
									<graphic>
										<VBox alignment="CENTER" prefHeight="200.0"
											prefWidth="100.0">
											<children>
												<ImageView fx:id="imgRegist">
													<image>
														<Image url="/view/Regist.png" />
													</image>
												</ImageView>
												<Label text="정보 등록" />
											</children>
										</VBox>
									</graphic>
								</Button>
								<Button fx:id="btnEdit">
									<graphic>
										<VBox alignment="CENTER" prefHeight="200.0"
											prefWidth="100.0">
											<children>
												<ImageView fx:id="imgEdit">
													<image>
														<Image url="/view/Edit.png" />
													</image>
												</ImageView>
												<Label text="정보 수정" />
											</children>
										</VBox>
									</graphic>
								</Button>
								<Button fx:id="btnDelete">
									<graphic>
										<VBox alignment="CENTER" prefHeight="200.0"
											prefWidth="100.0">
											<children>
												<ImageView fx:id="imgDelete">
													<image>
														<Image url="/view/Delete.png" />
													</image>
												</ImageView>
												<Label text="정보 삭제" />
											</children>
										</VBox>
									</graphic>
								</Button>
							</children>
						</HBox>
						<HBox alignment="CENTER" prefHeight="110.0" prefWidth="410.0">
							<padding>
								<Insets top="10.0" />
							</padding>
							<spacing>30.0</spacing>
							<children>
								<Button fx:id="btnSearch">
									<graphic>
										<VBox alignment="CENTER" prefHeight="200.0"
											prefWidth="100.0">
											<children>
												<ImageView fx:id="imgSearch">
													<image>
														<Image url="/view/Search.png" />
													</image>
												</ImageView>
												<Label text="정보 검색" />
											</children>
										</VBox>
									</graphic>
								</Button>
								<Button fx:id="btnImgPath">
									<graphic>
										<VBox alignment="CENTER" prefHeight="200.0"
											prefWidth="100.0">
											<children>
												<ImageView fx:id="imgPathSearch">
													<image>
														<Image url="/view/SearchPath.png" />
													</image>
												</ImageView>
												<Label text="사진첨부" />
											</children>
										</VBox>
									</graphic>
								</Button>
								<Button fx:id="btnExit">
									<graphic>
										<VBox alignment="CENTER" prefHeight="200.0"
											prefWidth="100.0">
											<children>
												<ImageView fx:id="imgExit">
													<image>
														<Image url="/view/Exit.png" />
													</image>
												</ImageView>
												<Label text="메인으로" />
											</children>
										</VBox>
									</graphic>
								</Button>
							</children>
						</HBox>
					</children>
				</VBox>
			</children>
		</HBox>
		<HBox alignment="CENTER" prefHeight="500.0" prefWidth="1850.0">
			<children>
				<TableView fx:id="tableView" prefHeight="450.0"
					prefWidth="1850.0" />
			</children>
		</HBox>
	</children>
</VBox>
