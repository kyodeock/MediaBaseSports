<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.ComboBox?>

<VBox alignment="CENTER" stylesheets="/view/playerinfostyle.css"
	xmlns:fx="http://javafx.com/fxml" prefWidth="1800.0" prefHeight="800.0">
	<spacing>5.0</spacing>
	<Button fx:id="btnLogo">
		<graphic>
			<VBox alignment="CENTER" prefHeight="200.0" prefWidth="120.0">
				<children>
					<ImageView fx:id="imgLogo">
						<image>
							<Image url="/view/backgroundImg.png" />
						</image>
					</ImageView>
					<Label text="초기화 하시려면 로고를 클릭하세요" />
				</children>
			</VBox>
		</graphic>
	</Button>
	<HBox alignment="CENTER">
		<spacing>20.0</spacing>
		<Label text="선수명 : " prefWidth="120.0" />
		<TextField fx:id="txtP_name" prefWidth="150.0" />
		<Label text="상대 구단 : " prefWidth="120.0" />
		<TextField fx:id="txtV_Club" prefWidth="150.0"
			promptText="상대 구단명" />
		<Label text="출전 시간 : " prefWidth="120.0" />
		<TextField fx:id="txtAppsTime" prefWidth="150.0"
			promptText="출전 시간" />
		<Label text="평점 : " prefWidth="120.0" />
		<TextField fx:id="txtAppsRate" prefWidth="150.0"
			promptText="경기 평점" />
		<Label text="볼터치 횟수 : " prefWidth="120.0" />
		<TextField fx:id="txtBallTouch" prefWidth="150.0"
			promptText="볼터치 횟수" />
	</HBox>
	<HBox alignment="CENTER">
		<spacing>20.0</spacing>
		<Label text="패스 시도 횟수 : " prefWidth="120.0" />
		<TextField fx:id="txtTryPass" prefWidth="150.0"
			promptText="패스 시도 횟수" />
		<Label text="키패스 횟수 : " prefWidth="120.0" />
		<TextField fx:id="txtKeyPass" prefWidth="150.0"
			promptText="키패스 횟수" />
		<Label text="패스 성공 횟수 : " prefWidth="120.0" />
		<TextField fx:id="txtSuccessPass" prefWidth="150.0"
			promptText="패스 성공 횟수" />
		<Label text="패스 성공률 : " prefWidth="120.0" />
		<TextField fx:id="txtPSuccessRate" prefWidth="150.0"
			promptText="패스 성공률" />
		<Label text="어시스트 : " prefWidth="120.0" />
		<TextField fx:id="txtAssists" prefWidth="150.0"
			promptText="어시스트" />
	</HBox>
	<HBox alignment="CENTER">
		<spacing>20.0</spacing>
		<Label text="드리블 시도횟수 : " prefWidth="120.0" />
		<TextField fx:id="txtTryDribble" prefWidth="150.0"
			promptText="드리블 시도 횟수" />
		<Label text="드리블 성공횟수 : " prefWidth="120.0" />
		<TextField fx:id="txtSuccessDribble" prefWidth="150.0"
			promptText="드리블 성공 횟수" />
		<Label text="드리블 성공률 : " prefWidth="120.0" />
		<TextField fx:id="txtDSuccessRate" prefWidth="150.0"
			promptText="드리블 성공률" />
		<Label text="슈팅 시도횟수 : " prefWidth="120.0" />
		<TextField fx:id="txtTryShots" prefWidth="150.0"
			promptText="슈팅시도횟수" />
		<Label text="유효슈팅횟수 : " prefWidth="120.0" />
		<TextField fx:id="txtShotOnTarget" prefWidth="150.0"
			promptText="유효슈팅 횟수" />
	</HBox>
	<HBox alignment="CENTER">
		<spacing>20.0</spacing>
		<Label text="득점 수 : " prefWidth="120.0" />
		<TextField fx:id="txtGoal" prefWidth="150.0"
			promptText="득점 수" />
		<Label text="공중볼경합횟수 : " prefWidth="120.0" />
		<TextField fx:id="txtTryArielDual" prefWidth="150.0"
			promptText="공중볼 경합 횟수" />
		<Label text="공중볼경합 승리횟수 : " prefWidth="120.0" />
		<TextField fx:id="txtSuccessArielDual" prefWidth="150.0"
			promptText="공중볼 경합 승리 횟수" />
		<Label text="패스차단 시도횟수 : " prefWidth="120.0" />
		<TextField fx:id="txtTryCutPass" prefWidth="150.0"
			promptText="패스차단 시도 횟수" />
		<Label text="패스차단 성공횟수 : " prefWidth="120.0" />
		<TextField fx:id="txtSuccessCutPass" prefWidth="150.0"
			promptText="패스차단 성공 횟수" />
	</HBox>
	<HBox alignment="CENTER">
		<spacing>20.0</spacing>
		<Label text="태클 시도횟수 : " prefWidth="120.0" />
		<TextField fx:id="txtTryTackle" prefWidth="150.0"
			promptText="태클 시도 횟수" />
		<Label text="태클 성공횟수 : " prefWidth="120.0" />
		<TextField fx:id="txtSuccessTackle" prefWidth="150.0"
			promptText="태클 성공 횟수" />
		<Label text="태클 성공률 : " prefWidth="120.0" />
		<TextField fx:id="txtTSuccessRate" prefWidth="150.0"
			promptText="태클 성공률" />
		<Label text="선방횟수 : " prefWidth="120.0" />
		<TextField fx:id="txtTrySave" prefWidth="150.0"
			promptText="선방 횟수" />
		<Label text="실점 수 : " prefWidth="120.0" />
		<TextField fx:id="txtGainsGoal" prefWidth="150.0"
			promptText="실점 수" />
	</HBox>
	<HBox alignment="CENTER">
		<spacing>20.0</spacing>
		<Label text="경기일자 : " prefWidth="120.0" />
		<DatePicker fx:id="dpGameDate" prefWidth="150.0" />
		<Label text="홈 / 원정 : " prefWidth="120.0" />
		<ComboBox fx:id="cbHandAway" prefWidth="150.0"
			promptText="선택" />
		<Label text="" prefWidth="120.0" />
		<Label text="" prefWidth="150.0" />
		<Label text="" prefWidth="120.0" />
		<Label text="" prefWidth="150.0" />
		<Label text="" prefWidth="120.0" />
		<Label text="" prefWidth="150.0" />
	</HBox>
	<HBox alignment="CENTER">
		<TableView fx:id="tableView" prefWidth="1750.0" />
	</HBox>
	<HBox alignment="CENTER">
		<Button fx:id="btnCalc">
			<graphic>
				<VBox alignment="CENTER" prefHeight="200.0" prefWidth="120.0">
					<children>
						<ImageView fx:id="imgCalc">
							<image>
								<Image url="/view/calculator.png" />
							</image>
						</ImageView>
						<Label text="기록별 성공률 계산" />
					</children>
				</VBox>
			</graphic>
		</Button>
		<Button fx:id="btnRegist">
			<graphic>
				<VBox alignment="CENTER" prefHeight="200.0" prefWidth="120.0">
					<children>
						<ImageView fx:id="imgRegist">
							<image>
								<Image url="/view/Regist.png" />
							</image>
						</ImageView>
						<Label text="경기별 기록 등록" />
					</children>
				</VBox>
			</graphic>
		</Button>
		<Button fx:id="btnCancel">
			<graphic>
				<VBox alignment="CENTER" prefHeight="200.0" prefWidth="120.0">
					<children>
						<ImageView fx:id="imgExit">
							<image>
								<Image url="/view/Exit.png" />
							</image>
						</ImageView>
						<Label text="메인으로" />
					</children>
				</VBox>
			</graphic>
		</Button>
	</HBox>
</VBox>

