<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<VBox alignment="CENTER" stylesheets="/view/playerinfostyle.css"
	xmlns:fx="http://javafx.com/fxml" prefWidth="800.0" prefHeight="400.0">
	<!-- TODO Add Nodes -->
	<ImageView fx:id="imgLogo">
		<image>
			<Image url="/view/backgroundImg.png" />
		</image>
	</ImageView>
	<ImageView fx:id="imgContract">
	</ImageView>
	<Button fx:id="btnCancel">
		<graphic>
			<VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
				<children>
					<ImageView fx:id="imgExit">
						<image>
							<Image url="/view/Exit.png" />
						</image>
					</ImageView>
					<Label text="메인으로" />
				</children>
			</VBox>
		</graphic>
	</Button>
</VBox>

